{
  "version": 2,
  "builds": [
    {
      "src": "api/voice.py",
      "use": "@vercel/python"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/voice.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/voice.py"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
